{
    "compilerOptions": {
        "baseUrl": ".",
            "skipLibCheck": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "module": "esnext",
        "target": "ES2020",
        "allowJs": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "preserve",
        "incremental": true,
        "importHelpers": true,
        "isolatedModules": true,
        "strictNullChecks": true,
    "lib": [
        "ES2020",
        "dom",
        "dom.iterable",
        "esnext"
    ],
    },
    "include": [
        "**/*.ts"
    ]
}